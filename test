python src/main.py --config configs/Rerank/evqa_experiments/evqa_decoder.jsonnet --mode train --reset --override  --opts train.trainer_paras.limit_val_batches=5 train.trainer_paras.limit_train_batches=10 train.trainer_paras.max_epochs=1
