python src/main.py --config configs/Rerank/initial_experiments/okvqa_full_context_rerank.jsonnet --mode train --experiment_name DEBUGDUMMY --reset --override  --opts train.trainer_paras.limit_val_batches=2 train.trainer_paras.limit_train_batches=5 train.trainer_paras.max_epochs=1
python src/main.py --config configs/Rerank/initial_experiments/okvqa_full_context_rerank.jsonnet --mode test --experiment_name DEBUGDUMMY --reset --override  --opts test.trainer_paras.limit_test_batches=1 test.trainer_paras.max_epochs=1 test.batch_size=3